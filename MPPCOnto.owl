                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An Author of some writings, recorded with his generic individual informations. __entity belonging to the Author Sub-Ontology__</rdfs:comment>
        <rdfs:label>Author</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/francescomonzillo/ontologies/2023/6/MPPCOnto#AuthorAssignment -->

    <owl:Class rdf:about="http://www.semanticweb.org/francescomonzillo/ontologies/2023/6/MPPCOnto#AuthorAssignment">
